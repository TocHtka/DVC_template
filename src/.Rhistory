library(imager)
files <- list.files("../data/nl-mean", pattern="*.jpg", full.names=TRUE)
df <- data.frame(img = character(), mean=double(), std=double(), stringsAsFactors=FALSE)
for (file in files){
im <- load.image(file)
plot(im)
m <- mean(im)
s <- std(im)
df <- rbind(df, file, m, s)
}
